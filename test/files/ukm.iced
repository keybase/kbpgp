
{ukm,util} = require '../..'

exports.test_kb = (T, cb) ->
  sig = ""
  [_, body1] = ukm.get_sig_body { armored : sig }
  body0 = new Buffer sig, 'base64'
  T.assert util.bufeq_fast(body0, body1), "same value came back"
  cb()

